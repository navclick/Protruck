extends template

block pageVars
	- var pageTitle   = "Dashboard Analytics"
	- var activeMenu  = "dashboard-analytics"
	- var activeParentMenu = "dashboard"

block page-header

//- Include page content in page block
append content
	include ../contents/dashboard-analytics.html
	include ../contents/sidebar/right-sidebar.html

block footer
	include ../templates/sections/footer/dashboard-analytics-footer

//- Vendor CSS
//------------------------------
//- Add Vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/jquery-jvectormap/jquery-jvectormap.css')

//- Page CSS
//------------------------------
//- Add custom page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/css'+rtl+'/pages/dashboard.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/sparkline/jquery.sparkline.min.js')
	script(src=app_assets_path + '/vendors/chartjs/chart.min.js')
	script(src=app_assets_path + '/vendors/jquery-jvectormap/jquery-jvectormap.min.js')
	script(src=app_assets_path + '/vendors/jquery-jvectormap/jquery-jvectormap-world-mill-en.js')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	//- Dashboard custom js
	script(src=app_assets_path + '/js/scripts/dashboard-analytics.js', type='text/javascript')
	script(src=app_assets_path + '/js/scripts/vectormap-script.js', type='text/javascript')