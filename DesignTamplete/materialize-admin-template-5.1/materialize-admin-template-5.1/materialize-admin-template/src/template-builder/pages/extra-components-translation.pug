extends template

block pageVars
	- var pageTitle = "Translation"
	- var pageSubTitle = ""
	- var description = "The standard font Material Design uses is Roboto. We have included the font files with our framework.."
	- var activeParentMenu = "extra-components"
	- var activeMenu = "extra-components-translation"

block breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"}, {url:"#",name:"Extra Components"}, {name:"Translation"}])

//- Include page content in page block
append content
	include ../contents/extra-components-translation.html
	include ../contents/sidebar/right-sidebar.html
	include ../contents/sidebar/fab-menu.html

//- Vendor css
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href=app_assets_path + '/vendors/flag-icon/css/flag-icon.min.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src=app_assets_path + '/vendors/jquery-cookies/jquery.cookies.min.js', type='text/javascript')
	script(src=app_assets_path + '/vendors/translator/jqueryTranslator.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src=app_assets_path + '/js/scripts/extra-components-translation.js', type='text/javascript')